{
  "version": "0.4",
  "name": "speed",
  "description": "GPS Speedometer",
  "launch_path": "/speedometer/",
  "icons": {
    "16": "/speedometer/img/icon-16.png",
    "48": "/speedometer/img/icon-48.png",
    "128": "/speedometer/img/icon-128.png"
  },
  "developer": {
    "name": "Ricky Rosario (r1cky)",
    "url": "http://rickyrosario.com/"
  },
  "installs_allowed_from": ["https://marketplace.firefox.com"],
  "appcache_path": "/speedometer/manifest.appcache",
  "permissions": {
    "geolocation": {
      "description": "Used to calculate speed based on GPS changes."
    }
  },
  "locales": {
  },
  "default_locale": "en"
}
